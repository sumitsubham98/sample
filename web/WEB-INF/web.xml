<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>myservlet</servlet-name>
        <servlet-class>com.practice.MyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.practice.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminFetching</servlet-name>
        <servlet-class>com.practice.AdminFetching</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteData</servlet-name>
        <servlet-class>com.practice.DeleteData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditData</servlet-name>
        <servlet-class>com.practice.EditData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditDatabase</servlet-name>
        <servlet-class>com.practice.EditDatabase</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>myservlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminFetching</servlet-name>
        <url-pattern>/AdminFetching</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteData</servlet-name>
        <url-pattern>/DeleteData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditData</servlet-name>
        <url-pattern>/EditData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditDatabase</servlet-name>
        <url-pattern>/EditDatabase</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
